{
  "name": "frontend",
  "version": "1.8.1",
  "description": "Opinionated Next.js template configured with TypeScript, Material UI, Redux Toolkit and other devtools",
  "license": "MIT",
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "Material UI",
    "framer-motion",
    "redux",
    "redux-toolkit",
    "rtk-query",
    "jest",
    "eslint",
    "prettier",
    "lint-staged",
    "husky",
    "commitlint",
    "msw"
  ],
  "bugs": {
    "url": "https://github.com/uangpintar/nextjs-template/issues"
  },
  "homepage": "https://github.com/uangpintar/nextjs-template",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "check-all": "yarn type-check && yarn lint && yarn format",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --write .",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@fontsource/roboto": "^4.5.8",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "@reduxjs/toolkit": "^1.8.6",
    "framer-motion": "^7.3.6",
    "next": "^12.3.1",
    "next-api-handler": "^0.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-redux": "^8.0.4",
    "react-toastify": "^9.1.2",
    "firebase-frameworks": "^0.7.0",
    "firebase-functions": "^3.23.0",
    "firebase-admin": "^11.0.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "main": "server.js",
  "engines": {
    "node": "16"
  }
}